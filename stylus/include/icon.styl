.icon
    background currentColor
    display: inline-block
    height 1em
    -webkit-mask $mk = center center no-repeat
    mask $mk
    vertical-align top
    width @height
    for name, path in icons
        &.icon-{name}
            -webkit-mask-image: $mki = url(mediaPath + "/icons/" + path)
            mask-image: $mki
    img
        display none